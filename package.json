{
	"name": "nnp",
	"private": true,
	"scripts": {
		"build": "nuxt build && node afterbuild",
		"stagebuild": "env-cmd -f .env.stage nuxt build --trace-deprecation && node afterbuild",
		"livebuild": "env-cmd -f .env.live nuxt build --trace-deprecation && node afterbuild",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"dev": "bash ./sh/dev.sh",
		"deploy": "bash ./sh/deploy.sh",
		"clean": "npx nuxi cleanup && npx rimraf node_modules",
		"pm2": "npx pm2 start ecosystem.config.cjs --env production --no-daemon"
	},
	"dependencies": {
		"@azure/storage-blob": "^12.25.0",
		"@babel/core": "^7.24.9",
		"@babel/plugin-transform-modules-commonjs": "^7.24.8",
		"@babel/preset-env": "^7.24.8",
		"@fontsource/roboto": "^5.0.14",
		"@pinia/nuxt": "^0.5.1",
		"@types/node": "^18.17.5",
		"@vueuse/core": "^11.0.3",
		"applicationinsights": "^2.9.5",
		"dayjs": "^1.11.10",
		"env-cmd": "^10.1.0",
		"gsap": "^3.12.5",
		"nuxt": "^3.13.2",
		"nuxt-security": "^2.0.0",
		"pinia": "^2.2.2",
		"pretendard": "^1.3.9",
		"sass": "^1.80.1",
		"swiper": "^11.1.1",
		"terser": "^5.36.0",
		"universal-cookie": "^7.1.4",
		"vue": "^3.4.21",
		"vue-dompurify-html": "^5.1.0",
		"vue-router": "^4.3.0"
	}
}
